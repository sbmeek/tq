{
    "presets": [
        "react-app"
    ],
    "plugins": [
        [
        "react-css-modules",
        {
            "webpackHotModuleReloading": true,
            "autoResolveMultipleImports": true,
            "generateScopedName": 
            "[name]__[local]__[hash:base64:5]",
            // "[hash:base64:6]",
        }
        ]
    ]
}